<% @notifications.each do |notification|%>
<div class="user-view clearfix ">
  <%= link_to user_path(notification.visiter_id) do %>
    <%= attachment_image_tag notification.visiter, :image, :fill, 50, 50, fallback: "no_image.jpg", size: "50x50", class:"profile-img-circle" %>
  <% end %>
  <%= notification_admin_form(notification) %><span class="moderate-font"><%= " (#{time_ago_in_words(notification.created_at)} 前)" %></span>
</div>
<% end %>